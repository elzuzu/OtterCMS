{
  "appId": "com.indi-suivi",
  "productName": "Indi-Suivi",
  "directories": {
    "output": "release-builds"
  },
  "files": [
    "dist/**/*",
    "main.js",
    "preload.js"
  ],
  "extraFiles": [
    {
      "from": "config/app-config.json",
      "to": "config/app-config.json"
    }
  ],
  "win": {
    "target": ["portable", "zip"],
    "icon": "src/assets/app-icon.ico"
  },
  "forceCodeSigning": false
}
