# Configuration Node.js optimisée pour le plugin SQL
# Configuration de cache et performance
npm_config_cache_max=300000000
npm_config_cache_lock_retries=10
npm_config_cache_lock_wait=10000

# Désactiver la télémétrie npm
npm_config_disable_progress=true

# Configuration spécifique SQL
npm_config_build_from_source=false
